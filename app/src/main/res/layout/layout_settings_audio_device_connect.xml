<?xml version="1.0" encoding="utf-8"?><!--File: AULUXA Mobile - Settings - Main.psd, width:1440, height:2560-->
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <View
            android:id="@+id/background_top_navigation"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/shape_background_top_menu"
            android:visibility="gone"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/background_subtitle"
            android:layout_width="0dp"
            android:layout_height="45dp"
            android:background="@color/headerTitleBackgroundColor"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/background_top_navigation" />

        <View
            android:id="@+id/settingsmain_settings_bg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/shape_background_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/background_subtitle"
            app:layout_constraintVertical_bias="1.0" />

        <!--path: settings_syncdata, top: 565, bottom: 1959, left: 887, right: 270-->
        <TextView
            android:id="@+id/settings_title_settings"
            style="@style/ButtonText"
            fontPath="Fonts/Lato/Lato-Regular.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:text="CONNECT"
            android:textColor="@color/headerTitleTextColor"
            android:textSize="@dimen/header_font_size"
            app:layout_constraintBottom_toBottomOf="@+id/background_subtitle"
            app:layout_constraintLeft_toLeftOf="@+id/background_subtitle"
            app:layout_constraintRight_toRightOf="@+id/background_subtitle"
            app:layout_constraintTop_toTopOf="@+id/background_subtitle" />

        <ImageView
            android:id="@+id/btnBackSettings"
            android:layout_width="24dp"
            android:layout_height="45dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:contentDescription=""
            android:padding="@dimen/distance_smaller"
            android:scaleType="fitCenter"
            android:src="@drawable/icon_back"
            app:layout_constraintBottom_toBottomOf="@+id/background_subtitle"
            app:layout_constraintLeft_toLeftOf="@+id/background_subtitle"
            app:layout_constraintTop_toTopOf="@+id/background_subtitle"
            app:layout_constraintVertical_bias="0.51" />

        <EditText
            android:id="@+id/password_field_setuplogin_setuplogin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:background="@drawable/setup7_textfield_setup7"
            android:contentDescription=""
            android:drawableLeft="@drawable/login_password_icon"
            android:drawablePadding="10dp"
            android:gravity="center"
            android:hint="XXXXXXXXXX"
            android:inputType="textPassword"
            android:textColor="@color/auluxaGreen"
            android:textColorHint="@color/auluxaGreen"
            android:textSize="14sp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/emailaddre_field_setuplogin_setuplogin"
            app:layout_constraintRight_toRightOf="@+id/emailaddre_field_setuplogin_setuplogin"
            app:layout_constraintTop_toBottomOf="@+id/emailaddre_field_setuplogin_setuplogin" />

        <EditText
            android:id="@+id/emailaddre_field_setuplogin_setuplogin"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/setup7_textfield_setup7"
            android:contentDescription=""
            android:drawableLeft="@drawable/login_username_icon"
            android:drawablePadding="10dp"
            android:gravity="center"
            android:hint="SETUP@AULUXA.COM"
            android:inputType="textEmailAddress"
            android:textColor="@color/auluxaGreen"
            android:textColorHint="@color/auluxaGreen"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="@+id/settingsmain_settings_bg"
            app:layout_constraintRight_toRightOf="@+id/settingsmain_settings_bg"
            app:layout_constraintTop_toBottomOf="@+id/tvSelectDeviceIrNode" />

        <TextView
            android:id="@+id/tvSelectDeviceIrNode"
            fontPath="Fonts/Lato/Lato-Regular.ttf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="LOGIN"
            android:textAllCaps="true"
            android:textColor="@color/BgDarker"
            android:textSize="16sp"
            app:layout_constraintLeft_toLeftOf="@+id/settingsmain_settings_bg"
            app:layout_constraintRight_toRightOf="@+id/settingsmain_settings_bg"
            app:layout_constraintTop_toTopOf="@+id/settingsmain_settings_bg" />

        <TextView
            android:id="@+id/forgetands_signup_setuplogin_setuplogin"
            style="@style/ButtonText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="16dp"
            android:padding="5dp"
            android:text="@string/forgetands_signup_setuplogin_setuplogin"
            android:textColor="@color/auluxaGreen"
            android:textSize="16sp"
            app:layout_constraintLeft_toLeftOf="@+id/settingsmain_settings_bg"
            app:layout_constraintRight_toRightOf="@+id/settingsmain_settings_bg"
            app:layout_constraintTop_toBottomOf="@+id/btnConnect" />

        <Button
            android:id="@+id/btnConnect"
            style="@style/Widget.AppCompat.Button.Borderless"
            fontPath="Fonts/Lato/Lato-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/auluxa_button_selector"
            android:minHeight="@dimen/button_min_height"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:text="CONNECT"
            android:textColor="@color/white"
            app:layout_constraintLeft_toLeftOf="@+id/settingsmain_settings_bg"
            app:layout_constraintRight_toLeftOf="@+id/btnCancel"
            app:layout_constraintTop_toBottomOf="@+id/password_field_setuplogin_setuplogin" />

        <Button
            android:id="@+id/btnCancel"
            style="@style/Widget.AppCompat.Button.Borderless"
            fontPath="Fonts/Lato/Lato-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/auluxa_button_selector"
            android:elevation="0dp"
            android:minHeight="@dimen/button_min_height"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:text="CANCEL"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/btnConnect"
            app:layout_constraintLeft_toRightOf="@+id/btnConnect"
            app:layout_constraintRight_toRightOf="@+id/settingsmain_settings_bg"
            app:layout_constraintTop_toTopOf="@+id/btnConnect" />

        <Button
            android:id="@+id/btnAudioConnect"
            style="@style/Widget.AppCompat.Button.Borderless"
            fontPath="Fonts/Lato/Lato-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:background="#3C66C4"
            android:drawableLeft="@drawable/fb_icon"
            android:minHeight="@dimen/button_min_height"
            android:paddingBottom="0dp"
            android:paddingLeft="16dp"
            android:paddingTop="0dp"
            android:text="SIGN IN WITH FACEBOOK"
            android:textColor="@color/white"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/settingsmain_settings_bg"
            app:layout_constraintRight_toRightOf="@+id/settingsmain_settings_bg"
            app:layout_constraintTop_toBottomOf="@+id/forgetands_signup_setuplogin_setuplogin" />

        <Button
            android:id="@+id/btnSignInGplus"
            style="@style/Widget.AppCompat.Button.Borderless"
            fontPath="Fonts/Lato/Lato-Regular.ttf"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="24dp"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:background="#CF4332"
            android:drawableLeft="@drawable/g_plus"
            android:minHeight="@dimen/button_min_height"
            android:paddingBottom="0dp"
            android:paddingLeft="16dp"
            android:paddingTop="0dp"
            android:text="SIGN IN WITH GOOGLE"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/settingsmain_settings_bg"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/settingsmain_settings_bg"
            app:layout_constraintRight_toRightOf="@+id/settingsmain_settings_bg"
            app:layout_constraintTop_toBottomOf="@+id/btnAudioConnect"
            app:layout_constraintVertical_bias="0.0" />

    </android.support.constraint.ConstraintLayout>
</layout>