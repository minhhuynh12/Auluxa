<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="presenter"
            type="net.dirox.auluxa.login.LoginResultViewContract.Presenter"/>
    </data>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/setup9_bg_setup9"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/setup9_bg_setup9"
            android:contentDescription=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.85"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp" />

        <ImageView
            android:id="@+id/setup9_vectorsmar_setup9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setup9_vectorsmar_setup9"
            android:contentDescription=""
            app:layout_constraintLeft_toLeftOf="@+id/setup9_bg_setup9"
            app:layout_constraintRight_toRightOf="@+id/setup9_bg_setup9"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintTop_toTopOf="@+id/setup9_bg_setup9"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.55"
            app:layout_constraintBottom_toTopOf="@+id/textView_button_setup9_setup9" />

        <Button
            android:id="@+id/loginbtn_button_setup9_setup9"
            style="@style/Widget.AppCompat.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/auluxa_button_selector"
            android:contentDescription=""
            android:minHeight="@dimen/button_min_height"
            android:onClick="@{()-> presenter.loginButtonClicked()}"
            android:paddingLeft="50dp"
            android:paddingRight="50dp"
            android:text="LOGIN"
            android:textSize="14sp"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.9" />

        <TextView
            android:id="@+id/textView_button_setup9_setup9"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:text="THE SETUP PROCESS HAS BEEN"
            app:layout_constraintLeft_toLeftOf="@+id/setup9_vectorsmar_setup9"
            app:layout_constraintRight_toRightOf="@+id/setup9_vectorsmar_setup9"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/setup9_bg_setup9"
            android:layout_marginBottom="317dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="COMPLETEED"
            app:layout_constraintBottom_toBottomOf="@+id/setup9_bg_setup9"
            app:layout_constraintLeft_toLeftOf="@+id/textView_button_setup9_setup9"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/textView_button_setup9_setup9"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView_button_setup9_setup9"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />

    </android.support.constraint.ConstraintLayout>
</layout>