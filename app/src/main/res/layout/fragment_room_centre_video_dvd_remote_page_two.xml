<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent"
        android:orientation="vertical"
        android:padding="25dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:orientation="horizontal"
            android:weightSum="4">

            <LinearLayout
                android:id="@+id/llRed"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <ImageView
                    android:id="@+id/tvRed"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/red_dot"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llGreen"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center"
                tools:ignore="MissingPrefix">

                <ImageView
                    android:id="@+id/tvGreen"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/green"
                    android:textSize="11sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llYellow"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <ImageView
                    android:id="@+id/tvYellow"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/yello"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llBlue"
                android:layout_width="70dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <ImageView
                    android:id="@+id/tvBlue"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/blue"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llCustom1"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCustom1"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CUSTOM1"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCustom2"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCustom2"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CUSTOM2"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCustom3"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCustom3"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CUSTOM3"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llCustom4"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCustom4"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CUSTOM4"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCustom5"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCustom5"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CUSTOM5"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCustom6"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCustom6"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CUSTOM6"
                    android:textSize="11sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="50dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llOne"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvOne"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="1"
                    android:textColor="@color/auluxaGreen"
                    android:textSize="15sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llTwo"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvTwo"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="2"
                    android:textSize="15sp"
                    android:textColor="@color/auluxaGreen"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llThree"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvThree"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="3"
                    android:textColor="@color/auluxaGreen"
                    android:textSize="15sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llFour"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvFour"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="4"
                    android:textSize="15sp"
                    android:textColor="@color/auluxaGreen"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llFive"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvFive"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="5"
                    android:textSize="15sp"
                    android:textColor="@color/auluxaGreen"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llSix"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvSix"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="6"
                    android:textColor="@color/auluxaGreen"
                    android:textSize="15sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llSeven"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvSeven"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="7"
                    android:textSize="15sp"
                    android:textColor="@color/auluxaGreen"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llEight"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvEight"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="8"
                    android:textSize="15sp"
                    android:textColor="@color/auluxaGreen"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llNine"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvNine"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="9"
                    android:textColor="@color/auluxaGreen"
                    android:textSize="15sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/llCharacter"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvCharacter"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="-/--"
                    android:textSize="15sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llZero"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvZero"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="0"
                    android:textSize="15sp"
                    android:textColor="@color/auluxaGreen"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/llClear"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:background="@drawable/shape_background_btn_video"
                android:gravity="center">

                <TextView
                    android:id="@+id/tvClear"
                    fontPath="Fonts/Lato/Lato-Regular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="CLEAR"
                    android:textSize="15sp"
                    tools:ignore="MissingPrefix" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
