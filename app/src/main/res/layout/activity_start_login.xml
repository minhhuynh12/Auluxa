<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="presenter"
            type="net.dirox.auluxa.login.LoginStartActivityContract.Presenter"/>
    </data>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/background_popupfilte_setupmain"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/background_popupfilte_setupmain"
            android:contentDescription=""
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.50208333333333"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.5"/>


        <TextView
            android:id="@+id/activeon_demo_setupmain_navigation_demo"
            style="@style/ButtonText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableTop="@drawable/activeon_hg_setuplogin_navigation_demo"
            android:text="@string/activeon_demo_setupmain_navigation_demo"
            android:textColor="@color/black"
            android:textSize="10sp"
            android:drawablePadding="@dimen/pading_drawable"
            android:visibility="visible"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintHorizontal_bias="0.75"
            app:layout_constraintVertical_bias="0.9" />


        <TextView
            android:id="@+id/activeon_login_setupmain_navigation_login"
            style="@style/ButtonText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableTop="@drawable/activeon_bg_setupmain_navigation_login"
            android:onClick="@{() -> presenter.loginButtonClicked()}"
            android:drawablePadding="@dimen/pading_drawable"
            android:text="@string/activeon_login_setupmain_navigation_login"
            android:textColor="@color/black"
            android:visibility="visible"
            android:textSize="10sp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.9" />

        <TextView
            android:id="@+id/activeon_setup_setupmain_navigation_setup"
            style="@style/ButtonText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/pading_drawable"
            android:textSize="10sp"
            android:drawableTop="@drawable/activeon_bg_setuplogin_navigation_setup"
            android:onClick="@{() -> presenter.setupButtonClicked()}"
            android:text="SETTINGS"
            android:textColor="@color/black"
            android:visibility="visible"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.9"
            app:layout_constraintHorizontal_bias="0.25" />

        <ImageView
            android:id="@+id/logo_logo_setupmain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/logo_logo_setupmain"
            android:contentDescription=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.50034722222222"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.4021484375"/>
    </android.support.constraint.ConstraintLayout>
</layout>
